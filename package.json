{
  "name": "authingy",
  "version": "0.0.7",
  "private": false,
  "description": "A type-safe, minimal, opinionated OAuth library with plugin-based providers for JavaScript and TypeScript.",
  "keywords": [
    "auth",
    "github",
    "google",
    "oauth",
    "typescript"
  ],
  "homepage": "https://github.com/arikchakma/authingy",
  "bugs": "https://github.com/arikchakma/authingy/issues",
  "license": "MIT",
  "author": "Arik Chakma <arikchakma@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/arikchakma/authingy"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run --watch example/server.ts",
    "build": "tsdown",
    "test": "bun test",
    "format:write": "oxfmt",
    "format:check": "oxfmt --check ",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "oauth4webapi": "^3.8.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bumpp": "^10.3.2",
    "oxfmt": "^0.19.0",
    "oxlint": "^1.34.0",
    "tsdown": "^0.17.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
